{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "npm run build",
    "dev": "rimraf wwwroot/ && node scripts/dev.js",
    "start": "react-scripts start",
    "dtos": "x typescript",
    "build": "rimraf wwwroot/ && react-scripts build && (move build wwwroot || mv build wwwroot)",
    "publish": "npm run build && dotnet publish -c Release",
    "test": "react-scripts test --env=jsdom --watchAll",
    "test-coverage": "npm test -- --coverage",
    "eject": "react-scripts eject"
  },
  "proxy": "https://localhost:5001/",
  "dependencies": {
    "@servicestack/client": "^1.1.19",
    "@servicestack/react": "^1.0.8",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^12.20.13",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "bootstrap": "^5.0.1",
    "cross-fetch": "^3.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.5.4",
    "web-vitals": "^1.1.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
